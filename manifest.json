{
    "manifest_version": 3,
    "name": "Gallery Xhancer",
    "version": "1.0",
    "description": "Refocus Twitter Gallery Pages to display images full across the canvas.",
    "permissions": ["tabs", "webNavigation"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*"],
        "js": ["content.js"]
      }
    ]
    
  }