{
    "manifest_version": 3,
    "name": "My CSS Modifier",
    "version": "1.0",
    "description": "Modify CSS of webpages.",
    "permissions": ["tabs", "webNavigation"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
  }
    
  }